{
  "compilerOptions": {
    "target": "es2020",
    "strict": true,
    "preserveConstEnums": true,
    //"noEmit": true,
    "sourceMap": false,
    "module":"Node16",
    "moduleResolution":"Node16",
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true, 
    "isolatedModules": true, 
    "outDir": "dist"
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}